LOG_LEVEL=INFO

# Production Database (PostgreSQL)
DB_HOST=db
DB_USER=postgres
DB_PASS=secret
DB_NAME=postgres_db

# Test Database (PostgreSQL)
DB_TEST_HOST=db
DB_TEST_USER=postgres
DB_TEST_PASS=secret
DB_TEST_NAME=postgres_db_test

# Celery settings (for PostgreSQL result backend)
CELERY_BROKER_URL=redis://redis:6379/0
CELERY_RESULT_BACKEND=db+postgresql://postgres:secret@db:5432/postgres_db